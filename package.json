{
  "name": "PoECustomSoundtrack",
  "version": "1.6.1",
  "description": "A small app to replace path of exile music with that of diablo",
  "main": "main.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development electron main.js",
    "release": "electron-builder -p onTagOrDraft",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "PoECustomSoundtrack",
    "asar": "true",
    "win": {
      "publish": "github",
      "icon": "pietyd2.ico",
      "target": [
        "zip",
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": "true",
      "perMachine": "true"
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "author": "jareddr",
  "license": "CC0-1.0",
  "dependencies": {
    "electron-log": "^5.0.1",
    "electron-settings": "^4.0.4",
    "electron-updater": "^6.1.7",
    "file-tail": "^0.3.0",
    "ipc": "0.0.1",
    "ps-list": "^5.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.3"
  }
}
